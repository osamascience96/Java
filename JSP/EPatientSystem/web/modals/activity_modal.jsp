<!-- Modal -->
<div class="modal fade" id="activity_modal" tabindex="-1" aria-labelledby="activities_header" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="activities_header">My Activities</h5>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
          <div class="d-grid gap-2">
              <button class="btn btn-primary" type="button" data-bs-toggle="modal" data-bs-target="#available_modal" data-bs-dismiss="modal">Set my Availability</button>
              <button class="btn btn-info" type="button">View my Appointments</button>
          </div>
      </div>
      <div class="modal-footer">
        <div class="d-grid gap-2">  
            <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Close</button>
        </div>
      </div>
    </div>
  </div>
</div>